@typeparam TItem

<SfAccumulationChart Title="@TituloPrincipal" EnableBorderOnMouseMove="false" SelectionMode="AccumulationSelectionMode.Point">
    <AccumulationChartTooltipSettings Enable="true"></AccumulationChartTooltipSettings>
    <AccumulationChartSeriesCollection>
        <AccumulationChartSeries DataSource="@DatosMostrarAsObject" XName="@XName" YName="@YName" Name="@NombreTitulo">
            <AccumulationDataLabelSettings Visible="true">
                <AccumulationChartDataLabelFont Color="white"></AccumulationChartDataLabelFont>
            </AccumulationDataLabelSettings>
        </AccumulationChartSeries>
    </AccumulationChartSeriesCollection>
</SfAccumulationChart>